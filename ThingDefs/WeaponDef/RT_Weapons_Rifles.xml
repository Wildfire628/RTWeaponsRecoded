<!-- ==========================Inferno Rifle==================================== -->  
    <ThingDef ParentName="BaseBullet">
        <defName>Proj_InfernoRifle_Flame</defName>
        <label>Inferno Projectile</label>
        <graphicData>
            <texPath>Projectile/Flame</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>TransparentPostLight</shaderType>
        </graphicData>
        <thingClass>Projectile_Explosive</thingClass>
        <projectile>
            <speed>15</speed>
            <damageDef>Flame</damageDef>
            <damageAmountBase>5</damageAmountBase>
            <explosionRadius>1</explosionRadius>
            <preExplosionSpawnThingDef>Filth_Fuel</preExplosionSpawnThingDef>
            <preExplosionSpawnChance>0.2</preExplosionSpawnChance>
			<soundExplode>Flame_Burn</soundExplode>
		<ai_IsIncendiary>true</ai_IsIncendiary>	
        </projectile>
    </ThingDef>
    <ThingDef ParentName="RT_HeavyGun">
        <defName>Weapon_InfernoRifle_rt</defName>
        <label>Inferno Rifle</label>
        <description>Shots out fire projectiles!Low Damage but can cause massive chaos.Use with caution.</description>
        <graphicData>
            <texPath>Weapon/InfernoRifle</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <soundInteract>Interact_AssaultRifle</soundInteract>
		<costList>
			<Steel>130</Steel>
			<ComponentIndustrial>3</ComponentIndustrial>
		</costList>
        <statBases>
            <Mass>3</Mass>
			  <AccuracyTouch>0.90</AccuracyTouch>
			  <AccuracyShort>0.80</AccuracyShort>
			  <AccuracyMedium>0.45</AccuracyMedium>
			  <AccuracyLong>0.10</AccuracyLong>
            <RangedWeapon_Cooldown>5</RangedWeapon_Cooldown>
			<WorkToMake>85000</WorkToMake>
        </statBases>
    <tools>
	<li>
			<label>stock</label>
			<capacities>
			  <li>Blunt</li>
			</capacities>
			<power>9</power>
			<cooldownTime>1.8</cooldownTime>
		  </li>
		  <li>
			<label>barrel</label>
			<capacities>
			  <li>Blunt</li>
			  <li>Poke</li>
			</capacities>
			<power>10</power>
			<cooldownTime>1.9</cooldownTime>
	 </li>
    </tools>		
	<equippedStatOffsets>
		<MoveSpeed>-0.3</MoveSpeed>
	</equippedStatOffsets>	
        <verbs>
            <li>
                <verbClass>Verb_Shoot</verbClass>
                <hasStandardCommand>true</hasStandardCommand>
                <defaultProjectile>Proj_InfernoRifle_Flame</defaultProjectile>
                <warmupTime>3</warmupTime>
                <range>16</range>
                <burstShotCount>5</burstShotCount>
                <ticksBetweenBurstShots>9</ticksBetweenBurstShots>
				<forcedMissRadius>1</forcedMissRadius>			
                <soundCast>InfernoRifleShot</soundCast>
            </li>
        </verbs>
    </ThingDef>	

